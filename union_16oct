#include<iostream>
using namespace std;
union student{
 //string name;   union does not take string
 int age;
 int roll_no;
 bool sch;

 void display(){
 cout<<"age is "<<age<<endl<<"roll no "<<roll_no<<endl;}
 };
 int main(){

 student s1;
 //s1.name="Rohin";
 s1.age=13;
 s1.roll_no=23;
 s1.sch=true;
 s1.display();
 return 0;
 }
