/*#include<bits/stdc++.h>
using namespace std;


int main(){
    int a=10;
    int &b=a;
    cout<<a<<endl;
    cout<<b<<endl;
    a=a+10;
    cout<<b<<endl;
    b=b+10;
    cout<<a;
    
    return 0;
}*/



/*#include<bits/stdc++.h>
