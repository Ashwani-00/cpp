#include<bits/stdc++.h>
using namespace std;
void print(int num){
    if(num<-3){
        return ;
    }
    cout<<num+4<<" ";
    num--;
    print(num);
}
int main(){
    int num;
    cin>>num;
    print(num);
    return 0;
}
